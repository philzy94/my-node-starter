{
  "name": "nodejs-backend-starter",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc",
    "typeorm": "ts-node --esm typeorm.config.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/config/typeorm.config.ts src/migrations/AutoGenerated",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/config/typeorm.config.ts",
    "generate:controller": "node scripts/generate-controller.js",
    "generate:route": "node scripts/generate-route.js",
    "generate:service": "node scripts/generate-service.js",
    "generate:entity": "node scripts/generate-entity.js",
    "generate:middleware": "node scripts/generate-middleware.js",
    "generate:interface": "node scripts/generate-interface.js",
    "generate": "node scripts/generate.js",
    "generate:all": "node scripts/generate-all.js",
    "generate:some": "node scripts/generate-some.js",
    "generate:auth": "node scripts/generate-auth-scaffold.js",
    "generate:dto": "node scripts/generate-dto.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.25",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
